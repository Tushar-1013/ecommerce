<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" ahref="style.css" />

    <title>Home Page</title>
  </head>
  <body>
    <div id="product">
      <% products.forEach((product) => { %>
      <div class="holder">
        <img
          src="<%= product.img %>"
          height="50px"
          width="50px"
          alt="<%= product.name %>"
        />
        <h2><%= product.productname %></h2>
      </div>
      <% }) %>
    </div>
  </body>
</html>
